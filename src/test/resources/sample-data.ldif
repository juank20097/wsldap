# Base DN (distinguished name)
dn: dc=example,dc=com
objectClass: top
objectClass: domain
dc: example

# Organización
dn: o=Example_Organization,dc=example,dc=com
objectClass: top
objectClass: organization
o: Example Organization

# Unidad organizativa (OU)
dn: ou=People,o=Example_Organization,dc=example,dc=com
objectClass: top
objectClass: organizationalUnit
ou: People

# Primer usuario
dn: uid=john.doe,ou=People,o=Example_Organization,dc=example,dc=com
objectClass: top
objectClass: person
objectClass: organizationalPerson
objectClass: inetOrgPerson
cn: John Doe
sn: Doe
uid: john.doe
mail: john.doe@example.com
userPassword: 12345

